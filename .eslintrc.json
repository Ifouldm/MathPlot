{
 "plugins": [
        "p5js"
    ],
    "env": {
        "browser": true,
        "es2021": true,
        "p5js/p5": true
    },
    "extends": [
        "airbnb-base",
        "p5js",
        "plugin:p5js/p5"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off"
    }
}
